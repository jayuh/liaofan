  "license": "MIT",
  "main": "src/index.js",
  "scripts": {
    "dev": "nodemon src/index.js",
    "start": "npm run dev",
    "production": "cross-env NODE_ENV=production node bin/production.js",
    "debug": "npm run clean && npm run compile && node --debug app/app.js",
    "build": "npm test && npm run clean && npm run compile",
    "compile": "babel src/ --out-dir app/ --source-maps",
    "clean": "node ./node_modules/rimraf/rimraf.js -rf app/",
    "test": "mocha -u bdd --compilers js:babel-core/register",
    "watch": "babel src/ --out-dir app/ --source-maps --watch",
    "mock": "node node_modules/json-server/bin/index.js --watch db.json --port 8080",
    "lint": "node node_modules/eslint/bin/eslint.js ./src/** ./test/**",
    "db": "mongod --dbpath=./data",
    "error": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shenxiaolong/liaofan"
  },
  "dependencies": {
    "bluebird": "^3.4.6",
    "co": "^4.6.0",
    "import-dir": "0.0.1",
    "joi": "^9.0.4",
    "koa": "^2.0.0",
    "koa-bodyparser": "^3.2.0",
    "koa-compress": "^1.0.9",
    "koa-convert": "^1.2.0",
    "koa-cors": "0.0.16",
    "koa-csrf": "^3.0.3",
    "koa-flash": "^1.0.0",
    "koa-generic-session": "^1.11.3",
    "koa-generic-session-mongo": "^0.2.5",
    "koa-json": "^1.1.3",
    "koa-logger": "^1.3.0",
    "koa-onerror": "^2.0.0",
    "koa-router": "^7.0.1",
    "koa-session": "^3.3.1",
    "koa-static": "^2.0.0",
    "koa-swig": "^2.2.0",
    "koa-views": "^5.1.2",
    "lodash": "^4.16.1",
    "log4js": "^0.6.38",
    "mongoose": "^4.6.1"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-preset-es2015-node5": "^1.2.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.11.6",
    "chokidar": "^1.6.0",
    "colors": "^1.1.2",
    "cross-env": "^2.0.1",
    "eslint": "^3.6.0",
    "eslint-config-standard": "^6.0.1",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-standard": "^2.0.0",
    "nodemon": "^1.10.2",
    "rimraf": "^2.5.4",
    "should": "^11.1.0",
    "supertest": "^2.0.0"
  }
}
